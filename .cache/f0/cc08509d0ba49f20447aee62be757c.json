n         top: 5px;\n      }\n   }\n}\n\n#skills {\n   height: 60vh;\n   color: $black;\n   font-weight: 600;\n   display: flex;\n   flex-direction: column;\n   justify-content: center;\n   align-items: center;\n   transform: translateY(-3%);\n   background: #fff;\n   #skills-header{\n      font-size: 3.4rem;\n      color: $brandYellow;\n   }\n   #skills-blurb {\n      text-align: center;\n      width: 50%;\n      font-size: 1.3rem;\n      margin-bottom: 3%;\n   }\n   #skills-group {\n      font-size: 1.2rem;\n      width: 60%;\n      display: flex;\n      flex-direction: row;\n      justify-content: space-evenly;\n      h2 {\n         color: $brandYellow;\n         font-weight: 900;\n         padding-left: 10px;\n      }\n      ul {\n         padding-top: 10px;\n      }\n      #front-end h2{\n         border-left: $accentGreen 4px solid;\n      }\n      #source-control h2{\n         border-left: $accentBlue 4px solid;\n      }\n      #uiux h2{\n         border-left: $accentPurple 4px solid;\n      }\n      #soft-skills h2{\n         border-left: $accentOrange 4px solid\n      }\n   }\n}\n\n#projects {\n   height: 80vh;\n   background: $black;\n   display: flex;\n   justify-content: center;\n   align-items: center;\n   flex-direction: column;\n   h1 {\n      color: #fff;\n      font-size: 3.4rem;\n      margin-bottom: 5%;\n   }\n   #project-sort {\n      display: flex;\n      flex-flow: row wrap;\n      justify-content: space-evenly;\n      width: 80%;\n   }\n   .project-box {\n      position: relative;\n      width: 30vw;\n      height: 20vw;\n      margin: -1%;\n      border: 10px outset $brandYellow;\n      background-color: $brandBlue;\n      h2 {\n         color: white;\n         font-size: 1.8rem;\n         position: absolute;\n         left: 1%;\n         bottom: 1%;\n         transition: ease 0.8s;\n         z-index: 1;\n      }\n      img {\n         height: 100%;\n         transform: translateX(25%);\n         z-index: -99;\n      }\n   }\n   .project-overlay {\n      position: absolute;\n      top: 0;\n      left: 0;\n      right: 0;\n      bottom: 0;\n      height: 100%;\n      width: 100%;\n      opacity: 0;\n      transition: .7s ease;\n      background: rgba(66, 66, 66, 0.7);\n      color: black;\n      display: flex;\n      flex-flow: column nowrap;\n      justify-content: space-evenly;\n      align-items: center;\n      text-align: center;\n      .project-blurb {\n         font-size: 1.2rem;\n         font-weight: 800;\n         p {\n            transform: translateY(30%);\n         }\n      }\n   }\n   .project-box:hover .project-overlay {\n      opacity: 1;\n      transition: ease 0.7s;\n   }\n   .project-box:hover h2 {\n      opacity: 0;\n   }\n \n}\n","style\\_variables.scss":"// Colors\n$black: #111;\n$brandBlue: #144587;\n$brandYellow: #bc881f;\n$accentOrange: #e66210;\n$accentPurple: #802dd4;\n$accentGreen: #1ed510;\n$accentBlue: #0070dd;\n\n// Fonts\n$font1: 'Montserrat', sans-serif;\n\n\n// Tooling\n$testBorder1: 5px dashed #59E259;\n$testBorder2: 5px dashed red;"},"lineCount":null}},"error":null,"hash":"a623a30aa0001eceee100d77eef27f2b","cacheData":{"env":{}}}